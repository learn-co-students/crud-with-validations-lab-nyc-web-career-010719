<%= form_for @song do |f| %>
  <%= f.label 'Song title' %>
  <%= f.text_field :title %>

  <%= f.label 'Released' %>
  <%= f.check_box :released %>

  <%= f.label 'Release year' %>
  <%= f.number_field :release_year %>

  <%= f.label 'Artist name' %>
  <%= f.text_field :artist_name %>

  <%= f.label 'Genre' %>
  <%= f.text_field :genre %>

  <%= f.submit %>
<% end %>

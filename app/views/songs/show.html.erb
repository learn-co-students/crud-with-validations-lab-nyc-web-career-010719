<h1> <%= @song.title %> </h1>

<%= button_to "All Songs", songs_path, :method => "get"%>

<% if @song.released %>
<p>Released: <%= @song.release_year %> </p>
<% else %>
<p>Not yet released</p>
<% end %>

<p>Artist: <%= @song.artist_name %></p>
<p>Genre: <%= @song.genre %></p>

<%= button_to "Edit Song", edit_song_path, :method => "get"%>

<%= button_to "Delete Song", song_path, :method => "delete"%>
